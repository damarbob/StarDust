{
  "name": "damarbob/stardust",
  "description": "The EAV-over-JSON Library for CodeIgniter 4",
  "license": "MIT",
  "type": "library",
  "autoload": {
    "psr-4": {
      "StarDust\\": "src/"
    },
    "files": [
      "src/Helpers/stardust_helper.php"
    ]
  },
  "require": {
    "php": "^8.1",
    "codeigniter4/framework": "^4.0",
    "damarbob/starcore": "^0.2.0"
